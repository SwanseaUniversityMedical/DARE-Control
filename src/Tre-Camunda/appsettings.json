{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341",
    "SeqApiKey": "",
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Model.Validation": "Error",
        "System": "Warning",
        "Hangfire": "Warning"
      }
    }
  },
  "ZeebeBootstrap": {
    "Client": {
      "GatewayAddress": "127.0.0.1:26500"
    },
    "Worker": {
      "MaxJobsActive": 5,
      "TimeoutInMilliseconds": 500,
      "PollIntervalInMilliseconds": 50,
      "PollingTimeoutInMilliseconds": 1000,
      "RetryTimeoutInMilliseconds": 1000
    }
  },
  "LdapSettings": {
    "Host": "localhost", //ldap.camundaephemeral.local
    "Port": 1389,
    "AdminDn": "cn=admin,dc=camundaephemeral,dc=local",
    "AdminPassword": "admin",
    "BaseDn": "dc=camundaephemeral,dc=local",
    "UserOu": "ou=Users",
    "UseSSL": false
  },
  "VaultSettings": {
    "BaseUrl": "http://localhost:8200",
    "Token": "dev-only-token",
    "TimeoutSeconds": 30,
    "SecretEngine": "secret",
    "EnableRetry": true,
    "MaxRetryAttempts": 3
  },
  "MinioSettings": {
    "Url": "http://localhost:9002",
    "AccessKey": "minio",
    "SecretKey": "minio123",
    "BucketName": "testbucket",
    "AWSRegion": "us-east-1",
    "AWSService": "s3",
    "AttributeName": "policy",
    "AdminConsole": "localhost:9001"
  }
}

